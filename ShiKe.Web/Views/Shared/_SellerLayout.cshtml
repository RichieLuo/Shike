@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    @*<meta name="viewport" content="width=device-width" />*@
    <meta charset="utf-8">
    <title>@ViewBag.Title - 美食每刻</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/base.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/fileinput.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/non-responsive.css" rel="stylesheet" />
    <link href="~/css/Index/indexFooter.css" rel="stylesheet" />
    @Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body style="padding:0px;background:#fff;">
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/for-home-index.js"></script>
    <script src="~/js/for-Shop-Goods.js"></script>
    <script src="~/lib/bootstrap/dist/js/fileinput.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/locales/zh.js"></script>
    <script src="~/lib/jquery/dist/jquery-form.js"></script>

    @Html.Partial("_ForLayoutNav.cshtml")
    <div class="container disTop">
        <div class="row" style="height:80px;padding:0px 15px;">
            <div class="col-xs-3" style="padding:0" ;>
                <a href="#"><img src="~/images/logo-For-Seller.png" style="width:220px;height:60px;" alt="Alternate Text" /></a>
            </div>

        </div>
        <nav class="navbar" role="navigation" style="border-radius:0px;background-color:#C72E00">
            <div class="navbar-header">
                <a class="navbar-brand" href="/" style="background-color:transparent; color:#fff">首页</a>
            </div>
            <div>
                <ul class="nav navbar-nav">
                    <li><a href="../../Home/Unfinished" style="background-color:transparent; color:#fff">规则中心</a></li>
                    <li><a href="../../Home/Unfinished" style="background-color:transparent; color:#fff">安全中心</a></li>
                    <li><a href="../../Home/Unfinished" style="background-color:transparent; color:#fff">食刻大学</a></li>
                    <li><a href="../../Home/Unfinished" style="background-color:transparent; color:#fff">商家论坛</a></li>
                    <li><a href="../../Home/Unfinished" style="background-color:transparent; color:#fff">服务中心</a></li>
                    <li><a href="../../Home/Unfinished" style="background-color:transparent; color:#fff">问题反馈</a></li>
                </ul>
            </div>
        </nav>
    </div>
    <div class="container" style="padding:30px 0">
        <div class="col-xs-2 sidebar" style="padding-left:0">
            <div class="list-group">
                <span class="list-group-item active text-center" style="background-color: #c72e00;border:0; border-radius: 0;height: 50px;font-size: 20px;padding-top: 12px"> 店铺管理 </span>
                <a href="../../Home/ShopForBusiness" class="list-group-item" style="border-radius: 0"> 我的店铺首页 </a>
                <a href="../../BusinessBG/ShopInformation" class="list-group-item" style="border-radius: 0"> 店铺信息 </a>
                <a href="../../Home/Unfinished" class="list-group-item" style="border-radius: 0"> 系统消息 </a>
            </div>
            <div class="list-group">
                <span class="list-group-item active text-center" style="background-color: #c72e00;border:0; border-radius: 0;height: 50px;font-size: 20px;padding-top: 12px"> 商品管理 </span>
                <a href="../../BusinessBG/CreateOrEdit" class="list-group-item" style="border-radius: 0"> 添加商品 </a>
                <a href="../../BusinessBG/GoodsList" class="list-group-item" style="border-radius: 0"> 出售中的商品 </a>
                <a href="../../BusinessBG/SpecialtyExamine" class="list-group-item" style="border-radius: 0"> 查看审核的商品 </a>
                <a href="../../Home/Unfinished" class="list-group-item" style="border-radius: 0"> 已下架的商品 </a>
            </div>
            <div class="list-group">
                <span class="list-group-item active text-center" style="background-color: #c72e00;border:0; border-radius: 0;height: 50px;font-size: 20px;padding-top: 12px"> 交易管理 </span>
                <a href="../BusinessBG/OrderList" class="list-group-item" style="border-radius: 0"> 所有订单 </a>
                <a href="../BusinessBG/OrderList?selectObject=待付款" class="list-group-item" style="border-radius: 0"> 待付款的订单 </a>
                <a href="../BusinessBG/OrderList?selectObject=待发货" class="list-group-item" style="border-radius: 0"> 待发货的订单 </a>
                <a href="../BusinessBG/OrderList?selectObject=待收货" class="list-group-item" style="border-radius: 0"> 待收货的订单 </a>
                <a href="../../Home/Unfinished" class="list-group-item" style="border-radius: 0"> 失败的订单 </a>
                <a href="../../Home/Unfinished" class="list-group-item" style="border-radius: 0"> 我的评价 </a>
            </div>
        </div>
            <div class="col-xs-10 main">

                @RenderBody()

            </div>
        </div>
    @Html.Partial("_ForLayoutFooter.cshtml")
    
</body>
</html>
