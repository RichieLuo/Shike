@model ShiKe.ViewModels.BusinessOrganization.PersonVM
<div>
    <button type="button" class="btn btn-success pull-right" onclick="gotoList('')">返回列表</button>
    <h2 class="sub-header">查看人员明细数据</h2>
</div>

<div class="row">
    <div class="col-md-9">
        <div class="row">
            <div class="col-md-3">姓名：</div>
            <div class="col-md-9">@Model.Name</div>
        </div>
        <div class="row">
            <div class="col-md-3">电子邮件：</div>
            <div class="col-md-9">@Model.Email</div>
        </div>
        <div class="row">
            <div class="col-md-3">移动电话：</div>
            <div class="col-md-9">@Model.MobileNumber</div>
        </div>
    </div>
    <div class="col-md-3">这里是准备放照片的</div>
</div>
